{
  "name": "@vazgentigranich/parlor-glimmer-application-pipeline",
  "version": "0.15.7",
  "description": "Tooling for developing Glimmer standalone apps with ember-cli",
  "contributors": [
    "Dan Gebhardt <dan@cerebris.com>",
    "Tom Dale <tom@tomdale.net>",
    "Robbie Pitts <me@robbie-pitts.com>"
  ],
  "scripts": {
    "build": "ember build",
    "changelog": "lerna-changelog",
    "prepare": "yarn run build",
    "postpublish": "git push origin master --tags",
    "pretest": "ember build",
    "test": "yarn run problems && mocha dist/tests --recursive",
    "test:debug": "ember build && mocha debug dist/tests --recursive",
    "start": "ember test --server",
    "problems": "tsc -p tsconfig.json --noEmit"
  },
